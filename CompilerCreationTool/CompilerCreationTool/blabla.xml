<Language>
    <Grammar>
        <Production left="S">
            <Symbol name="Int" type="Terminal"/>
            <Symbol name="Plus" type="Terminal"/>
            <Symbol name="Int" type="Terminal"/>
            <Symbol name="EOF" type="Terminal"/>
        </Production>
    </Grammar>
    <Patterns>
        <Pattern name="Int" origin="([A-Za-z_])([A-Za-z0-9])*" isEnding="false"/>
        <Pattern name="Plus" origin="Plus" isEnding="false"/>
        <Pattern name="EOF" origin="EOF" isEnding="true"/>
    </Patterns>
    <Actions/>
</Language>
